{
  "name": "protocol-dapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "next lint && prettier --check \"**/*.{ts,tsx,js,jsx,md,json}\" && prisma validate && tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\" && eslint --fix && prisma format",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --headless"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ],
    "**/*.prisma": [
      "prisma format"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^5.22.2",
    "@types/node": "20.11.30",
    "@types/react": "18.2.69",
    "@types/react-dom": "18.2.22",
    "@vercel/analytics": "^1.1.3",
    "@vercel/blob": "^0.22.2",
    "@vercel/speed-insights": "^1.0.10",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "ethers": "^6.11.1",
    "gray-matter": "^4.0.3",
    "next": "14.1.4",
    "prisma": "^5.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "typescript": "^5.3.3",
    "viem": "^2.7.12",
    "wagmi": "^2.5.12",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.1.4",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.8.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1"
  },
  "resolutions": {
    "string-width": "^4.2.0"
  }
}
