{
  "name": "protocol-dapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "beforebuild": "npx prisma generate",
    "build": "yarn beforebuild && next build && yarn predeploy",
    "predeploy": "npx prisma migrate deploy",
    "start": "next start",
    "prepare": "husky install",
    "lint": "next lint && prettier --check \"**/*.{ts,tsx,js,jsx,md,json}\" && prisma validate && tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\" && eslint --fix && prisma format",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "db:generate": "dotenv -e .env.local -- npx prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ],
    "**/*.prisma": [
      "prisma format"
    ]
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.8.1",
    "@prisma/adapter-neon": "^5.9.1",
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^5.22.2",
    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "@vercel/analytics": "^1.1.3",
    "@vercel/postgres": "^0.7.2",
    "@vercel/speed-insights": "^1.0.10",
    "dotenv": "^16.4.5",
    "encoding": "^0.1.13",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "gray-matter": "^4.0.3",
    "lokijs": "^1.5.12",
    "next": "13.4.4",
    "pino-pretty": "^10.0.1",
    "prisma": "^5.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "styled-components": "^6.0.3",
    "typescript": "^5.3.3",
    "viem": "^2.7.12",
    "wagmi": "^2.5.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "dotenv-cli": "^7.3.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.8",
    "tsx": "^4.7.1"
  }
}
